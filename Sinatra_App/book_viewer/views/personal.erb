<h1><%= @user[0].to_s %></h1>

<h3>Email: </h3>
  <p><%= @user[1][:email] %></p>

<h3>Interests: </h3>
  <p><%= @user[1][:interests].join(", ") %></p>

<h4>View other users:</h4>
<ul>
  <% @users.each do |user| %>
    <% name = user[0].to_s%>
    <% next if name == @user[0].to_s %>
    <li><a href="/users/<%= name %>"><%= name %></a></li>
  <% end %>
</ul>